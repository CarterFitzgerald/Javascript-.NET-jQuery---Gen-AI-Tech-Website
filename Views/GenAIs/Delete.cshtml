@model Final_Assignment_Carter_Fitzgerald.Models.GenAIs

@{
    ViewData["Title"] = "Delete";
}

<div id="delete-container">
    <h1 id="delete-heading-2">Delete GenAI Website</h1>
    <hr id="yellow-line-8">
    <h3 id="delete-heading">Are you sure you want to delete this website?</h3>
    <hr id="yellow-line-8">
    <div>
        <dl class="row">
            <dt class = "col-sm-2 delete-label">
                @Html.DisplayNameFor(model => model.GenAIName)
            </dt>
            <dd class = "col-sm-10 delete-label">
                @Html.DisplayFor(model => model.GenAIName)
            </dd>
            <dt class = "col-sm-2 delete-label">
                @Html.DisplayNameFor(model => model.Summary)
            </dt>
            <dd class = "col-sm-10 delete-label">
                @Html.DisplayFor(model => model.Summary)
            </dd>
            <dt class = "col-sm-2 delete-label">
                Image Filename
            </dt>
            <dd class = "col-sm-10 delete-label">
                @Html.DisplayFor(model => model.ImageFileName)
            </dd>
            <dt class = "col-sm-2 delete-label">
                Anchor Link
            </dt>
            <dd class = "col-sm-10 delete-label">
                @Html.DisplayFor(model => model.AnchorLink)
            </dd>
            <dt class = "col-sm-2 delete-label">
                @Html.DisplayNameFor(model => model.Likes)
            </dt>
            <dd class = "col-sm-10 delete-label">
                @Html.DisplayFor(model => model.Likes)
            </dd>
        </dl>
    
        <form asp-action="Delete" id="line">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a id="web-p" asp-action="Index"><u>Back to List</u></a>
        </form>
    </div>
</div>
